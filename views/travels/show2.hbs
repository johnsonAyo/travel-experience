<div class="row">
    <div class="col s12 m8">
        <h3>{{travel.title}}
        </h3>
        <div class="card story">
            <div class="card-content">
                <span class="card-title">{{formatDate date 'MMMM Do YYYY, h:mm:ss a'}}</span>
                {{{travel.description}}}
            </div>
        </div>
    </div>
    <div class="col s12 m4 profile">
        <div class="card center-align">
            <div class="card-content">
                <span class="card-title">{{travel.user.displayName}}</span>
                <img src="{{travel.user.image}}" class="circle responsive-img img-small">
            </div>
            <div class="card-action">
                <a href="/user/{{travel.user._id}}"> More From {{travel.user.firstName}}</a>
            </div>
        </div>
    </div>
    <div class="col s12 m12">
        <p>{{travel.notes}}
        </p>
    </div>
</div>
<div class="card-image">
      {{#each travel.images}}
        <img src="{{this}}" height="600px"> <br><br>
        {{/each}}
</div>